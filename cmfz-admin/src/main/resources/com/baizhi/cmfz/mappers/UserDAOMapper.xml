<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "../mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.cmfz.dao.UserDAO">
     <select id="countBySex" resultType="java.lang.Integer">
         SELECT count(*)
         FROM `user`
         WHERE sex=#{sex}
     </select>

</mapper>
